<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:contentDescription="@string/account"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="1dp"
    android:fillViewport="true"
    >


    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".Events">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="@string/login_image"
            android:foregroundGravity="left"
            android:src="@drawable/background_decoration"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="-2dp" />


        <TextView
            android:id="@+id/headingTextView"
            android:layout_width="202dp"
            android:layout_height="98dp"
            android:fontFamily="@font/poppins_family"
            android:text="@string/plantation_events"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/imageView7"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.041" />


        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="403dp"
            android:layout_height="65dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toTopOf="@+id/imageView7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/headingTextView"
            app:layout_constraintVertical_bias="0.0">

            <Button
                android:id="@+id/submitbtn"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginStart="30dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/round_button"
                android:fontFamily="@font/poppins_family"
                android:text="@string/add_events"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="18sp" />

        </LinearLayout>

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/imageView11"
            android:layout_width="360dp"
            android:layout_height="167dp"
            android:layout_marginTop="-130dp"
            android:layout_marginBottom="-130dp"
            android:scaleType="centerCrop"
            android:src="@drawable/plantation_program"
            android:onClick="clickNew"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.353"
            app:shapeAppearanceOverlay="@style/roundImageView1" />

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/imageView12"
            android:layout_width="360dp"
            android:layout_height="167dp"
            android:layout_marginTop="-130dp"
            android:layout_marginBottom="-130dp"
            android:scaleType="centerCrop"
            android:src="@drawable/seeds"
            android:onClick="clickNew1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.552"
            app:shapeAppearanceOverlay="@style/roundImageView1" />

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/imageView13"
            android:layout_width="360dp"
            android:layout_height="167dp"
            android:layout_marginTop="-130dp"
            android:layout_marginBottom="-130dp"
            android:scaleType="centerCrop"
            android:onClick="clickNew2"
            android:src="@drawable/planting_adverticing"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.515"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.749"
            app:shapeAppearanceOverlay="@style/roundImageView1" />

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/imageView14"
            android:layout_width="360dp"
            android:layout_height="167dp"
            android:layout_marginTop="-130dp"
            android:layout_marginBottom="-130dp"
            android:scaleType="centerCrop"
            android:onClick="clickNew3"
            android:src="@drawable/plant_distribution"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.515"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.95"
            app:shapeAppearanceOverlay="@style/roundImageView1" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="360dp"
            android:layout_height="167dp"
            android:background="#45FFFFFF"
            android:fontFamily="@font/poppins_family"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView11"
            app:layout_constraintEnd_toEndOf="@+id/imageView11"
            app:layout_constraintStart_toStartOf="@+id/imageView11"
            app:layout_constraintTop_toTopOf="@+id/imageView11"
            app:shapeAppearanceOverlay="@style/roundImageView1" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="360dp"
            android:layout_height="167dp"
            android:background="#35FFFFFF"
            android:fontFamily="@font/poppins_family"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView12"
            app:layout_constraintEnd_toEndOf="@+id/imageView12"
            app:layout_constraintStart_toStartOf="@+id/imageView12"
            app:layout_constraintTop_toTopOf="@+id/imageView12"
            app:shapeAppearanceOverlay="@style/roundImageView1"/>

        <TextView
            android:id="@+id/textView3"
            android:layout_width="360dp"
            android:layout_height="167dp"
            android:background="#35FFFFFF"
            android:fontFamily="@font/poppins_family"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView13"
            app:layout_constraintEnd_toEndOf="@+id/imageView13"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageView13"
            app:layout_constraintTop_toTopOf="@+id/imageView13"
            app:layout_constraintVertical_bias="0.0"
            app:shapeAppearanceOverlay="@style/roundImageView1" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="360dp"
            android:layout_height="167dp"
            android:background="#35FFFFFF"
            android:fontFamily="@font/poppins_family"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imageView14"
            app:layout_constraintStart_toStartOf="@+id/imageView14"
            app:layout_constraintTop_toTopOf="@+id/imageView14"
            app:shapeAppearanceOverlay="@style/roundImageView1"/>

        <TextView
            android:id="@+id/textView16"
            android:layout_width="234dp"
            android:layout_height="43dp"
            android:fontFamily="@font/poppins_family"
            android:text="Plant Distribution"
            android:textColor="@color/black"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView11"
            app:layout_constraintEnd_toEndOf="@+id/imageView11"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/imageView11"
            app:layout_constraintTop_toTopOf="@+id/imageView11"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="235dp"
            android:layout_height="46dp"
            android:fontFamily="@font/poppins_family"
            android:text="@string/seed_distribution"
            android:textColor="@color/black"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView12"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.865"
            app:layout_constraintStart_toStartOf="@+id/imageView12"
            app:layout_constraintTop_toBottomOf="@+id/imageView11"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_family"
            android:text="Plantation Program"
            android:textColor="@color/black"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView13"
            app:layout_constraintEnd_toEndOf="@+id/imageView13"
            app:layout_constraintHorizontal_bias="0.846"
            app:layout_constraintStart_toStartOf="@+id/imageView13"
            app:layout_constraintTop_toTopOf="@+id/imageView13"
            app:layout_constraintVertical_bias="1.0" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
